#include<iostream>
using namespace std;
int main()
 {
     int a,b;
cout<<"Enter 2 numbers for checking the greatest number\n";
cout<<"enter the valus of A\n";
cin>>a;
cout<<"enter the valus of B\n";
cin>>b;
if (a>b)
{
    cout<<"A is greater than B\n";
}
else
{
    cout<<"B is greater than A\n";
}

return 0;
 }
